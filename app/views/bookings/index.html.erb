<h1>here's all your bookings</h1>
<ul>
  <% @bookings.each do |booking| %>
    <li><%= link_to booking.item.name, item_path(booking.item_id) %></li>
    <li><%= booking.date_booked_start %></li>
    <li><%= booking.date_booked_end %></li>
    <%= link_to "Edit booking", edit_booking_path(booking) %>
    <%= link_to "Cancel booking",  delete_booking_path(booking), data: { turbo_method: :delete, turbo_confirm: "Are you sure you want to cancel this booking?" }%>
  <% end %>
</ul>
